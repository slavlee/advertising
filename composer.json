{
    "name": "slavlee/advertising",
    "type": "typo3-cms-extension",
    "description": "The best advertising extension for TYPO3 to monetarize you TYPO3 webpage.",
    "authors": [],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^12.4.8"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "require-dev": {
        "typo3/testing-framework": "^8.0.7"
    },
    "autoload": {
        "psr-4": {
            "Slavlee\\Advertising\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slavlee\\Advertising\\Tests\\": "Tests"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "advertising",
            "web-dir": ".Build/Web"
        }
    }
}